#include <stdio.h>

int main() {
    int n = 5;

    for (int i = 1; i <= n; i++) {

            for (int j = 1; j <= i; j++) {

                printf("%d",j);

            }

            for (int k = i + 1; k <= n; k++) {

             printf("1");

            }
        printf("\n");
    }

    return 0;
}
